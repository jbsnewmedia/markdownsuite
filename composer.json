    "license": "MIT",
    "keywords": ["markdown", "bootstrap", "parsedown", "commonmark", "documentation", "documentation-suite", "markdown-suite", "help", "help-suite"],
    "authors": [
        {
            "name": "Juergen Schwind - JBS New Media GmbH",
